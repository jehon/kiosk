    "puppeteer": "1.19.0",
    "randomport": "0.1.1",
    "start-server-and-test": "1.10.0",
    "watch-then-execute": "1.1.1"
  },
  "scripts": {
    "clean": "rm -fr parts tmp",
    "eslint": "eslint --ignore-path .gitignore --ignore-pattern /unused .",
    "eslint:fix": "npm run eslint -- --fix",
    "postinstall": "./apply-patches.sh",
    "start": "node --experimental-modules main.js -f tests/kiosk.yml",
    "start-server": "node --experimental-modules main.js --server-only -f tests/kiosk.yml",
    "start-server-for-gui-tests": "node --experimental-modules server.js -p 3002",
    "test": "npm run test-server && npm run test-client && npm run test-gui-with-server && npm run eslint",
    "test-client": "cd tests/client && karma start --single-run",
    "test-client-watch": "cd tests/client && karma start",
    "test-server": "node --experimental-modules tests/server/jasmine-run.mjs",
    "test-gui": "cd tests/gui && cypress run --browser chromium",
    "test-gui-with-server": "start-server-and-test start-server-for-gui-tests  http://localhost:3002 test-gui",
    "cypress-open": "cd tests/gui && cypress open",
    "cypress-result": "nautilus tmp/gui &",
    "depcheck": "depcheck"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
